<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.ninatran.represent.DetailView"
    tools:showIn="@layout/activity_detail_view">

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView17"
        android:src="@drawable/dianne"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sen. Dianne Feinstein"
        android:id="@+id/textView14"
        android:layout_marginStart="29dp"
        android:typeface="serif"
        android:textColor="#083D85"
        android:textSize="15dp"
        android:layout_alignTop="@+id/imageView17"
        android:layout_toEndOf="@+id/imageView17" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Party: Democrat"
        android:id="@+id/textView15"
        android:layout_below="@+id/textView14"
        android:layout_alignStart="@+id/textView14"
        android:typeface="serif"
        android:textColor="#040404" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="End Date: N/A"
        android:id="@+id/textView16"
        android:layout_below="@+id/textView15"
        android:layout_alignStart="@+id/textView15"
        android:typeface="serif"
        android:textColor="#FF040404" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView17"
        android:text="Senate Committee on Judiciary"
        android:layout_below="@+id/textView16"
        android:layout_alignStart="@+id/textView16"
        android:typeface="serif"
        android:textColor="#FF040404" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Bills:"
        android:id="@+id/textView18"
        android:layout_marginTop="33dp"
        android:textColor="#083D85"
        android:layout_below="@+id/imageView17"
        android:layout_alignParentStart="true"
        android:typeface="serif" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="S.2533 - Accelerated Revenue, Repayment, and Surface Water Storage Enhancement Act"
        android:id="@+id/textView19"
        android:layout_below="@+id/textView18"
        android:layout_alignParentStart="true"
        android:layout_marginTop="31dp"
        android:textColor="#FF040404"
        android:typeface="serif" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date Introduced: 02/10/2016"
        android:id="@+id/textView20"
        android:layout_below="@+id/textView19"
        android:layout_alignParentStart="true"
        android:textColor="#FF040404"
        android:typeface="serif" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="S.2442 - A bill to authorize the use of passenger facility charges at an airport previously associated with the airport at which the charges are collected."
        android:id="@+id/textView21"
        android:layout_below="@+id/textView20"
        android:layout_alignParentStart="true"
        android:layout_marginTop="38dp"
        android:textColor="#FF040404"
        android:typeface="serif" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date Introduced: 01/12/2016"
        android:id="@+id/textView22"
        android:layout_below="@+id/textView21"
        android:layout_alignParentStart="true"
        android:textColor="#FF040404"
        android:typeface="serif" />
</RelativeLayout>
